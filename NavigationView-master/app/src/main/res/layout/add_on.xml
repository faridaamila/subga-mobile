<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:id="@+id/relativeLayout16"
            android:background="@mipmap/search_back">

            <EditText
                android:layout_width="@dimen/_240sdp"
                android:layout_height="@dimen/_50sdp"
                android:id="@+id/search_bar"
                android:text="Search"
                android:textSize="@dimen/_12sdp"
                android:singleLine="true"
                android:scrollHorizontally="true"
                android:paddingLeft="@dimen/_10sdp"
                android:clickable="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_8sdp"
                android:paddingBottom="@dimen/_9sdp"
                android:textColorHint="#dfdbdb" />

            <ImageButton
                android:layout_width="@dimen/_37sdp"
                android:layout_height="@dimen/_50sdp"
                android:id="@+id/btn_search"
                android:layout_alignTop="@+id/search_bar"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_35sdp" />

            <Spinner
                android:layout_width="@dimen/_70sdp"
                android:layout_height="@dimen/_50sdp"
                android:id="@+id/spinner"
                android:textSize="@dimen/_10sdp"
                android:touchscreenBlocksFocus="true"
                android:prompt="@string/spinner"
                android:layout_toLeftOf="@+id/btn_search"
                android:layout_toStartOf="@+id/btn_search"
                android:clickable="true"
                android:layout_marginRight="@dimen/_4sdp"
                android:layout_alignBottom="@+id/btn_search"
                android:layout_marginBottom="@dimen/_2sdp"
                android:layout_alignTop="@+id/search_bar"
                android:layout_marginTop="@dimen/_1sdp"
                android:background="@mipmap/sort_baru" />
            <RadioGroup
                android:layout_width="@dimen/_150sdp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/rb_grup"
                android:layout_below="@+id/spinner"
                android:layout_alignLeft="@+id/search_bar"
                android:layout_alignStart="@+id/search_bar">

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GA INFO"
                    android:id="@+id/radioButton"
                    android:layout_gravity="center_horizontal" />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Subject"
                    android:id="@+id/radioButton2"
                    android:checked="false" />

            </RadioGroup>

        </RelativeLayout>

        <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_410sdp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/scrollView3"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/_2sdp"
            android:layout_marginRight="@dimen/_1sdp"
            android:layout_below="@+id/relativeLayout16">>

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/horizontalScrollView"
                android:layout_below="@+id/relativeLayout16"
                android:layout_centerHorizontal="true">


                <TableLayout
                    android:id="@+id/tableLayout1"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignTop="@+id/scrollView3"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">
                    <TableRow
                        android:layout_width="@dimen/_325sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_below="@+id/textView12"
                        android:layout_alignLeft="@+id/textView12"
                        android:layout_alignStart="@+id/textView12"
                        android:background="#d1d2d4">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="#"
                            android:id="@+id/textView13"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="5dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Issue Date"
                            android:id="@+id/textView14"
                            android:layout_marginTop="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginBottom="5dp"
                            android:layout_marginRight="30dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="GA Info"
                            android:id="@+id/textView15"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="110dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginBottom="5dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Subject"
                            android:id="@+id/textView16"
                            android:layout_marginRight="200dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginBottom="5dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hits"
                            android:id="@+id/textView17"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginBottom="5dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Refer"
                            android:id="@+id/textView18"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="110dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginBottom="5dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Action"
                            android:id="@+id/textView19"
                            android:layout_marginTop="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginBottom="5dp" />
                    </TableRow>
                </TableLayout>
            </HorizontalScrollView>
        </ScrollView>


    </RelativeLayout>

</LinearLayout>