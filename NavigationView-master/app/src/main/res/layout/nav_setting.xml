<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_120sdp"
        android:layout_below="@+id/relativeLayout7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="@dimen/_15sdp"
        android:layout_marginRight="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_10sdp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Settings"
            android:id="@+id/textView26"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textColor="#1F5AA6"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/_20sdp" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:id="@+id/edit_profile"
            android:layout_centerVertical="true"
            android:layout_alignRight="@+id/textView26"
            android:layout_alignEnd="@+id/textView26"
            android:background="@mipmap/background_setting" />

        <ImageView
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:id="@+id/imageView11"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@mipmap/profile" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Edit Profile"
            android:id="@+id/textView54"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView11"
            android:layout_toEndOf="@+id/imageView11"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginStart="@dimen/_20sdp" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:id="@+id/notifications"
            android:layout_below="@+id/edit_profile"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@mipmap/background_setting" />

        <ImageView
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:id="@+id/imageView12"
            android:background="@mipmap/notifications"
            android:layout_alignBottom="@+id/notifications"
            android:layout_alignLeft="@+id/imageView11"
            android:layout_alignStart="@+id/imageView11"
            android:layout_marginBottom="@dimen/_5sdp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Notifications"
            android:id="@+id/textView55"
            android:layout_alignBottom="@+id/notifications"
            android:layout_alignLeft="@+id/textView54"
            android:layout_alignStart="@+id/textView54"
            android:layout_marginBottom="@dimen/_10sdp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="@dimen/_15sdp"
        android:layout_marginRight="@dimen/_15sdp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Support"
            android:id="@+id/textView27"
            android:layout_marginTop="@dimen/_20sdp"
            android:textStyle="bold"
            android:textColor="#1F5AA6"/>

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:id="@+id/help_center"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@mipmap/background_setting" />

        <ImageView
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:id="@+id/imageView13"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:background="@mipmap/operator" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Contact Center"
            android:id="@+id/textView56"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView13"
            android:layout_toEndOf="@+id/imageView13"
            android:layout_marginLeft="@dimen/_20sdp" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:id="@+id/about"
            android:layout_below="@+id/help_center"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@mipmap/background_setting" />

        <ImageView
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:id="@+id/imageView14"
            android:layout_alignBottom="@+id/about"
            android:layout_alignLeft="@+id/imageView13"
            android:layout_alignStart="@+id/imageView13"
            android:layout_marginBottom="@dimen/_5sdp"
            android:background="@mipmap/info" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="About"
            android:id="@+id/textView57"
            android:layout_alignBottom="@+id/about"
            android:layout_alignLeft="@+id/textView56"
            android:layout_alignStart="@+id/textView56"
            android:layout_marginBottom="@dimen/_10sdp" />

    </RelativeLayout>

</LinearLayout>